{
  "cluster": {
    "name": "cluster-2",
    "zone": "australia-southeast1-a",
    "network": "default",
    "nodePools": [
      {
        "name": "default-pool",
        "initialNodeCount": 1,
        "config": {
          "machineType": "n1-standard-4",
          "imageType": "COS",
          "diskSizeGb": 50,
          "diskType": "pd-standard",
          "preemptible": false,
          "oauthScopes": [
            "https://www.googleapis.com/auth/cloud-platform"
          ]
        },
        "autoscaling": {
          "enabled": true,
          "minNodeCount": 1,
          "maxNodeCount": 4
        },
        "management": {
          "autoUpgrade": false,
          "autoRepair": true,
          "upgradeOptions": {}
        }
      }
    ],
    "loggingService": "logging.googleapis.com",
    "monitoringService": "monitoring.googleapis.com",
    "initialClusterVersion": "1.10.4-gke.2",
    "masterAuth": {
      "username": "admin",
      "clientCertificateConfig": {
        "issueClientCertificate": true
      }
    },
    "legacyAbac": {
      "enabled": false
    },
    "masterAuthorizedNetworksConfig": {
      "enabled": false,
      "cidrBlocks": []
    },
    "addonsConfig": {
      "kubernetesDashboard": {
        "disabled": true
      },
      "httpLoadBalancing": {
        "disabled": false
      },
      "networkPolicyConfig": {
        "disabled": true
      }
    },
    "networkPolicy": {
      "enabled": false,
      "provider": "CALICO"
    },
    "subnetwork": "default",
    "ipAllocationPolicy": {
      "useIpAliases": false
    },
    "locations": [
      "australia-southeast1-a",
      "australia-southeast1-b",
      "australia-southeast1-c"
    ]
  }
}